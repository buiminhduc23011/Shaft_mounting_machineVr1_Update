                                          CanUserResizeColumns="False"
                                          CanUserResizeRows="False"
                                          IsReadOnly="True"
                                          Grid.ColumnSpan="3">
                                    <DataGrid.Columns>
                                        <DataGridTextColumn Header="STT"
                                                            Binding="{Binding STT}"
                                                            Width="Auto">
                                            <DataGridTextColumn.ElementStyle>
                                                <Style TargetType="{x:Type TextBlock}">
                                                    <Setter Property="HorizontalAlignment"
                                                            Value="Center" />
                                                </Style>
                                            </DataGridTextColumn.ElementStyle>
                                        </DataGridTextColumn>
                                        <DataGridTextColumn Header="Model"
                                                            Binding="{Binding Model}"
                                                            Width="100" />
                                        <DataGridTextColumn Header="Mã Trục"
                                                            Binding="{Binding Truc}"
                                                            Width="200" />
                                        <DataGridTextColumn Header="Mã Roto"
                                                            Binding="{Binding Roto}"
                                                            Width="200" />
                                    </DataGrid.Columns>
